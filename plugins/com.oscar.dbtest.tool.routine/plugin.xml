<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.views">
      <view
            class="com.oscar.dbtest.tool.routine.views.ScriptsToRunView"
            id="com.oscar.dbtest.tool.routine.ScriptsToRunView"
            name="name"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="com.oscar.dbtest.tool.routine.main_toobar.runtest"
               label="功能测试">
            <command
                  commandId="com.oscar.dbtest.tool.routine.run"
                  icon="icons/run.png"
                  label="功能测试"
                  style="push">
            </command>
         </toolbar>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="com.oscar.dbtest.tool.routine.handler.RunTestHandler"
            commandId="com.oscar.dbtest.tool.routine.run">
      </handler>
   </extension>

</plugin>
